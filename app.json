{
  "expo": {
    "name": "taskcard-rn",
    "slug": "taskcard-rn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/tc-splash-with-bg-white.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.owais28.taskcardrn"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.owais28.taskcardrn"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ca2c61dc-0dac-4b9d-a7ab-fa2fbb339d1c"
      }
    }
  }
}